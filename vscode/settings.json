{
    "editor.fontSize": 15.5,
    "editor.tabSize": 2,
    "editor.fontLigatures": true,
    "editor.cursorBlinking": "smooth",
    "editor.minimap.autohide": true,
    // "editor.fontFamily": "JetBrains Mono NL",
    "editor.fontFamily": "JetBrains Mono",
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.glyphMargin": true,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.minimap.scale": 2,
    "editor.minimap.enabled": false,
    "editor.scrollbar.verticalScrollbarSize": 15,
    "editor.cursorWidth": 1,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "totalTypeScript.hideAllTips": false,
    "totalTypeScript.hideBasicTips": true,
    "workbench.productIconTheme": "feather-vscode",
    "workbench.colorTheme": "poimandres-storm",
    "workbench.iconTheme": "catppuccin-frappe",
    "workbench.tree.indent": 4,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.commandPalette.preserveInput": true,
    "workbench.layoutControl.type": "toggles",
    "workbench.activityBar.location": "top",
    "workbench.startupEditor": "none",
    "terminal.integrated.tabs.location": "left",
    "terminal.integrated.enableVisualBell": true,
    "terminal.integrated.tabs.defaultColor": "terminal.ansiCyan",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.cursorStyleInactive": "underline",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.profiles.osx": {
        "zsh": {
            "path": "zsh",
            "env": {
                "TMUX_PANE": null
            },
            "icon": "terminal",
        }
    },
    "terminal.integrated.defaultProfile.osx": "zsh",
    "typescript.inlayHints.parameterNames.enabled": "all",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "window.density.editorTabHeight": "compact",
    "scm.alwaysShowRepositories": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[env]": {
        "editor.defaultFormatter": "IronGeek.vscode-env"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "files.associations": {
        ".env.*": "env"
    },
    "totalTypeScript.hiddenTips": [
        "in-operator-narrowing",
        "passing-generics-to-types",
        "typeof",
        "as-const",
        "as-const-on-object",
        "omit-utility-type",
        "generic-slots-in-functions",
        "interface-with-generics",
        "interface-with-multiple-generics",
        "readonly-utility-type",
        "pick-utility-type",
        "type-alias-with-generics",
        "type-predicate",
        "partial-utility-type",
        "type-alias-with-multiple-generics",
        "parameters-utility-type",
        "mapped-type",
        "keyof",
        "conditional-type",
        "record-utility-type",
        "tuple-type",
        "exclude-utility-type",
        "bigint",
        "never-keyword",
        "non-null-expression",
        "returntype-utility-type",
        "nested-conditional-type",
        "infer",
        "awaited-utility-type",
        "remapping-in-mapped-type",
        "lowercase-utility-type",
        "required-utility-type",
        "nonnullable-utility-type",
        "extract-utility-type"
    ],
    "window.confirmBeforeClose": "always",
    "editor.accessibilitySupport": "off",
    "editor.lineNumbers": "relative",
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
    "github.copilot.editor.enableAutoCompletions": true,
    "window.zoomLevel": 0.5,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "editor.bracketPairColorization.enabled": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.wrapTabs": true,
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "vim.autoindent": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 450,
    "vim.highlightedyank.color": "rgba(124, 58, 237, 0.55)",
    "vim.useSystemClipboard": true,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.disableExtension": false,
    "vim.leader": "<Space>",
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "n",
                "h"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "H"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "<C-d>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "<C-u>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-f>"
            ],
            "after": [
                "<C-f>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-b>"
            ],
            "after": [
                "<C-b>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "n"
            ],
            "after": [
                "n",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "N"
            ],
            "after": [
                "N",
                "z",
                "z"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "workbench.panel.showLabels": false,
    "vim.showMarksInGutter": true,
    "tailwindCSS.experimental.classRegex": [
        [
            "cva\\(([^)]*)\\)",
            "[\"'`]([^\"'`]*).*?[\"'`]"
        ],
        [
            "cx\\(([^)]*)\\)",
            "(?:'|\"|`)([^']*)(?:'|\"|`)"
        ],
        [
            "clsx\\(.*?\\)(?!\\])",
            "(?:'|\"|`)([^\"'`]*)(?:'|\"|`)"
        ],
        [
            "(?:twMerge|twJoin)\\(([^;]*)[\\);]",
            "[`'\"`]([^'\"`;]*)[`'\"`]"
        ]
    ],
    "window.autoDetectColorScheme": false,
    "workbench.preferredLightColorTheme": "Catppuccin Latte",
    "workbench.preferredDarkColorTheme": "poimandres-storm"
}