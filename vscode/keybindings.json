// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "alt+cmd+e",
    "command": "toggleSearchRegex",
    "when": "searchInputBoxFocus && searchViewletVisible"
  },
  {
    "key": "alt+e",
    "command": "-toggleSearchRegex",
    "when": "searchInputBoxFocus && searchViewletVisible"
  },
  {
    "key": "alt+cmd+e",
    "command": "toggleFindRegex",
    "when": "editorFocus"
  },
  {
    "key": "alt+e",
    "command": "-toggleFindRegex",
    "when": "editorFocus"
  },
  {
    "key": "alt+cmd+z",
    "command": "editor.action.toggleWordWrap"
  },
  {
    "key": "alt+z",
    "command": "-editor.action.toggleWordWrap"
  }
]