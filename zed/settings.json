{
  "vim_mode": true,
  "tab_size": 2,
  "auto_update": false,
  "confirm_quit": false,
  "base_keymap": "VSCode",
  "ui_font_family": "JetBrains Mono NL",
  "relative_line_numbers": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "file_finder": {
    "modal_max_width": "medium"
  },
  "project_panel": {
    "button": false,
    "indent_size": 11,
    "indent_guides": {
      "show": "always"
    }
  },
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": true
  },
  "theme": {
    "mode": "system",
    "light": "Catppuccin Frappé",
    "dark": "Catppuccin Frappé"
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font Mono",
    "font_size": 12.5
  },
  "telemetry": {
    "metrics": false
  }
}
